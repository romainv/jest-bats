{
  "name": "jest-bats",
  "version": "0.1.3",
  "description": "Run Bats tests with Jest",
  "author": "Romain Vermeulen",
  "license": "MIT",
  "homepage": "https://github.com/romainv/jest-bats",
  "bugs": {
    "url": "https://github.com/romainv/jest-bats/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/romainv/jest-bats.git"
  },
  "keywords": [
    "jest",
    "bats",
    "bash",
    "runner",
    "test",
    "testing",
    "tap"
  ],
  "main": "jest-preset.js",
  "scripts": {
    "test": "jest",
    "upgrade": "multi-updates && npm install",
    "lint": "eslint"
  },
  "dependencies": {
    "bats": "^1.4.1",
    "create-jest-runner": "^0.7.0",
    "tap-parser": "^10.1.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "multi-updates": "^0.1.2",
    "prettier": "^2.3.2"
  },
  "peerDependencies": {
    "jest": "^26.0.0 || ^27.0.0"
  },
  "prettier": {
    "semi": false
  },
  "eslintConfig": {
    "rules": {
      "strict": 0,
      "prettier/prettier": "warn"
    },
    "env": {
      "node": true,
      "es2017": true,
      "browser": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "prettier"
    ]
  }
}
